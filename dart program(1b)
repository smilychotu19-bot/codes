void main() {
  // printing output
  print("Hello, Dart!");

  // variables
  int age = 20;
  double price = 19.99;
  String name = "Deekshitha";
  bool isStudent = true;

  print("Name: $name");
  print("Age: $age");
  print("Price: $price");
  print("Student: $isStudent");

  // List
  List<String> colors = ["Red", "Blue", "Green"];
  print("First Color: ${colors[0]}");

  // Loop
  for (var color in colors) {
    print("Color: $color");
  }
}



